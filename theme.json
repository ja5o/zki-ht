{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "960px",
			"wideSize": "1024px"
		},
		"color": {
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"name": "Background",
					"color": "#FAFAFA",
					"slug": "background"
				},
				{
					"name": "Foreground",
					"color": "#263238",
					"slug": "foreground"
				},
				{
					"name": "Accent",
					"color": "#1E88E5",
					"slug": "accent"
				},
				{
					"name": "Secondary",
					"color": "#FFEB3B",
					"slug": "secondary"
				},
				{
					"name": "Black",
					"color": "#000",
					"slug": "black"
				},
				{
					"name": "White",
					"color": "#fff",
					"slug": "white"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Figtree",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./build/fonts/figtree/figtree.woff2"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./build/fonts/figtree/figtree-italic.woff2"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./build/fonts/figtree/figtree-700.woff2"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./build/fonts/figtree/figtree-700italic.woff2"
							]
						}
					],
					"fontFamily": "\"Figtree\", ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
					"name": "Figtree",
					"slug": "default"
				},
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
					"name": "System Sans Serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "ui-serif, Georgia, serif",
					"name": "System Serif",
					"slug": "system-serif"
				},
				{
					"fontFamily": "ui-monospace, Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
					"name": "System Monospace",
					"slug": "system-monospace"
				}
			],
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"slug": "small",
					"name": "Small",
					"fluid": false,
					"size": "0.75rem"
				},
				{
					"slug": "medium",
					"name": "Medium",
					"fluid": false,
					"size": "1rem"
				},
				{
					"slug": "large",
					"name": "Large",
					"fluid": {
						"min": "1.44rem",
						"max": "1.777rem"
					},
					"size": "1.777rem"
				},
				{
					"slug": "x-large",
					"name": "X Large",
					"fluid": {
						"min": "1.728rem",
						"max": "2.369rem"
					},
					"size": "2.369rem"
				},
				{
					"slug": "xx-large",
					"name": "XX Large",
					"fluid": {
						"min": "2.074rem",
						"max": "3.157rem"
					},
					"size": "3.157rem"
				}
			]
		},
		"custom": {
			"heading-1": "clamp(2.986rem, 2.986rem + ((1vw - 0.2rem) * 5.964), 5.61rem)",
			"heading-2": "clamp(2.488rem, 2.488rem + ((1vw - 0.2rem) * 2.648), 4.209rem)",
			"heading-3": "var(--wp--preset--font-size--xx-large)",
			"heading-4": "var(--wp--preset--font-size--x-large)",
			"heading-5": "var(--wp--preset--font-size--large)",
			"heading-6": "clamp(1.2rem, 1.2rem + ((1vw - 0.2rem) * 0.205), 1.333rem)",
			"gutter": {
				"small": "10px",
				"medium": "20px",
				"large": "30px",
				"xlarge": "40px"
			},
			"stack": {
				"small": ".625rem",
				"medium": "1.25rem",
				"large": "1.875rem",
				"xlarge": "2.5rem"
			},
			"transition": {
				"ease": {
					"swing": "cubic-bezier(0.55, 0, 0.1, 1)",
					"swift": "cubic-bezier(0.4, 0, 0.2, 1)"
				},
				"time": {
					"default": "320ms",
					"slow": "600ms"
				}
			}
		},
		"blocks": {
			"core/group": {
				"spacing": {
					"margin": true,
					"padding": true
				},
				"border": {
					"color": true,
					"width": true,
					"style": true
				}
			}
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"size": "10px",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "20px",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "30px",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "40px",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "50px",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "60px",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "70px",
					"slug": "70",
					"name": "7"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--default)",
			"fontWeight": "400",
			"lineHeight": "1.2"
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--custom--gutter--large)",
				"left": "var(--wp--custom--gutter--large)"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--custom--heading-1)",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--custom--heading-2)",
					"fontWeight": "700"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--custom--heading-3)",
					"fontWeight": "700"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--custom--heading-4)",
					"fontWeight": "700"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--custom--heading-5)",
					"fontWeight": "700"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--custom--heading-6)",
					"fontWeight": "700"
				}
			},
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--foreground)",
						"text": "var(--wp--preset--color--accent)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--foreground)",
						"text": "var(--wp--preset--color--accent)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--foreground)",
						"text": "var(--wp--preset--color--accent)"
					}
				}
			}
		},
		"blocks": {}
	}
}
